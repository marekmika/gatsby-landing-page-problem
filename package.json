  "scripts": {
    "build": "yarn build:prepare && yarn build:en && yarn build:fr && yarn build:clean",
    "build:prepare": "rimraf public && rimraf dist && mkdirp dist",
    "build:clean": "rimraf public",
    "build:fr": "CURRENT_LANGUAGE=fr gatsby build",
    "build:en": "CURRENT_LANGUAGE=en gatsby build",
    "develop": "gatsby develop -H 0.0.0.0",
    "develop:en": "CURRENT_LANGUAGE=en yarn develop",
    "develop:fr": "CURRENT_LANGUAGE=fr yarn develop",
    "start:en": "yarn develop:en",
    "start:fr": "yarn develop:fr",
    "lint": "eslint src --ext tsx",
    "start:production": "node server/server.js",
    "start:production:fr": "CURRENT_LANGUAGE=fr yarn start:production",
    "start:production:en": "CURRENT_LANGUAGE=en yarn start:production"
  },
